{
  "expo": {
    "name": "accura",
    "slug": "accura",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "accura",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-accura.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "نحتاج للوصول إلى موقعك لتتبع التوصيل بشكل مباشر وعرض الطلبات القريبة منك",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "نحتاج للوصول إلى موقعك بشكل مستمر لتتبع التوصيل أثناء عملك كسائق",
        "NSLocationAlwaysUsageDescription": "نحتاج للوصول إلى موقعك بشكل مستمر لتتبع التوصيل أثناء عملك كسائق"
      },
      "bundleIdentifier": "com.accura.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION"
      ],
      "package": "com.accura.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "sounds": [
            "./assets/sounds/notification.mp3",
            "./assets/sounds/notification.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c3c8154e-50ce-44a1-b32a-37328206b4a7"
      }
    },
    "owner": "sabri-gaafar"
  }
}
